.model small           ; Use small memory model
.stack 100h            ; Allocate 256-byte stack

.data
num1 db 5          ; First number
num2 db 7          ; Second number
result db 0        ; Store subtraction result

.code

start:
mov ax, @data      ; Init data segment
mov ds, ax

mov al, num1       ; Load num1 into AL
sub al, num2       ; Subtract num2 from AL
mov result, al     ; Save result

mov ah, 4Ch        ; Exit program


end start              ; Program entry point




; [SOURCE]: D:\Microprocessor_LAb\mycode.asm
